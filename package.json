{
  "name": "lobo",
  "version": "0.2.0",
  "description": "Elm test runner",
  "keywords": [
    "elm",
    "lobo",
    "test"
  ],
  "bin": {
    "lobo": "./bin/lobo.js"
  },
  "engines": {
    "node": ">= 0.9"
  },
  "files": [
    "bin",
    "lib",
    "plugin",
    "runner"
  ],
  "author": "bja <code@benansell.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/benansell/lobo.git"
  },
  "license": "BSD-3-Clause",
  "dependencies": {
    "bluebird": "^3.4.6",
    "chalk": "^1.1.3",
    "chokidar": "^1.7.0",
    "commander": "^2.9.0",
    "fast-levenshtein": "^2.0.5",
    "lodash": "^4.16.4",
    "promptly": "^2.2.0",
    "shelljs": "^0.7.7",
    "tmp": "^0.0.31"
  },
  "devDependencies": {
    "chai": "^4.0.0",
    "eslint": "^3.7.1",
    "eslint-config-google": "^0.7.1",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1",
    "istanbul": "^0.4.5",
    "mocha": "^3.4.2",
    "mocha-lcov-reporter": "^1.2.0",
    "rewire": "^2.5.2",
    "rimraf": "^2.5.4",
    "sinon": "^2.3.2",
    "sinon-chai": "^2.10.0"
  },
  "scripts": {
    "clean": "rimraf node_modules",
    "test-lint": "eslint bin/ lib/ plugin/ test/ --ignore-pattern '**/elm-stuff/**'",
    "test-unit": "istanbul cover node_modules/mocha/bin/_mocha -- --reporter dot test/unit/**/**/*.test.js test/unit/**/**/**/*.test.js",
    "test-integration": "mocha test/integration/**/*.test.js test/integration/**/**/*.test.js -t 100000",
    "pretest": "npm run test-lint --silent",
    "test": "npm run test-unit --silent && npm run test-integration --silent"
  }
}
